                    <ref name="PCOPYRIGHT"/>
                    <ref name="PDEDICATION"/>
                    <ref name="PEARNINGS"/>
                    <ref name="PEDITIONS"/>
                    <ref name="PFIRSTLITERARYACTIVITY"/>
                    <ref name="PINFLUENCESHER"/>
                    <ref name="PLASTLITERARYACTIVITY"/>
                    <ref name="PLITERARYSCHOOLS"/>
                    <ref name="PMANUSCRIPTHISTORY"/>
                    <ref name="PMATERIALCONDITIONS"/>
                    <ref name="PMODEOFPUBLICATION"/>
                    <ref name="PMOTIVES"/>
                    <ref name="PNONBOOKMEDIA"/>
                    <ref name="PNONSURVIVAL"/>
                    <ref name="PPERFORMANCE"/>
                    <ref name="PPERIODICALPUBLICATION"/>
                    <ref name="PPLACEOFPUBLICATION"/>
                    <ref name="PPRESSRUN"/>
                    <ref name="PPRICE"/>
                    <ref name="PRARITIESFEATURESDECORATIONS"/>
