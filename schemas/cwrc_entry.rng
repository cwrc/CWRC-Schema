                    </choice>
                </attribute>
            </optional>
            <oneOrMore>
                <choice>
                    <text/>
                    <ref name="HEADING"/>
                    <ref name="P"/>
                    <ref name="DATASTRUCT"/>
                    <ref name="CHRONSTRUCT"/>
                    <ref name="BIBCITS"/>
                    <ref name="KEYWORDCLASS"/>
                    <ref name="SCHOLARNOTE"/>
                    <ref name="RESEARCHNOTE"/>
                    <ref name="SHORTPROSE"/>
