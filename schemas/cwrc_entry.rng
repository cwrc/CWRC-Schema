                    <ref name="RWRITINGMILESTONE"/>
                    <ref name="TCHARACTERIZATION"/>
                    <ref name="TCHARACTERNAME"/>
                    <ref name="TCHARACTERTYPEROLE"/>
                    <ref name="TGENRE"/>
                    <ref name="TGENREISSUE"/>
                    <ref name="TINTERTEXTUALITY"/>
                    <ref name="TMOTIF"/>
                    <ref name="TPLOT"/>
                    <ref name="TSETTINGDATE"/>
