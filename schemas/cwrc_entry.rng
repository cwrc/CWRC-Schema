    <define name="HEADING">
        <element name="HEADING">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">heading contains any type of heading, for example
                the title of a section, or the heading of a list, glossary, manuscript description, etc.</a:documentation>
            <attribute name="SUBTYPE">
                <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">The Subtype attribute allows you to indicate the heading
                    level, with values from 1-5.</a:documentation>
