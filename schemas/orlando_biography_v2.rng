                                    <value>SEXUALAWAKENINGFICTION</value>
                                    <value>SHORTSTORY</value>
                                    <value>SILVERFORKNOVEL</value>
                                    <value>SKETCH</value>
                                    <value>SKETCHBOOK</value>
                                    <value>SLAVENARRATIVE</value>
                                    <value>SOCIALSCIENCE</value>
                                    <value>SONG</value>
                                    <value>SONNET</value>
                                    <value>SPEECH</value>
                                    <value>STAGEREVIEW</value>
                                    <value>TESTIMONY</value>
