    <define name="RESEARCHNOTE">
        <element name="RESEARCHNOTE">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                >ResearchNote is available everywhere, and it allows us to comment among ourselves
                on the work at hand; not to be confused with scholarNotes that will be readable by
                end users, the contents of this element will be seen only by project
                members.</a:documentation>
            <optional>
                <attribute name="annotationId">
                    <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                        >Provides a unique annotation identifier for the element bearing the
                        attribute.</a:documentation>
                </attribute>
            </optional>
            <optional>
                <attribute name="offsetId">
                    <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                        >Designates the parent tag from which the offset is being
