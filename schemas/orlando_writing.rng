        <element name="RECEPTION">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Reception
                is one of the three largest buckets in the writing documents (in addition to
                TextualFeatures and Production). Reception charts the effects and results of an
                individual's writing, and the responses of self and others to an individual's
                writing.</a:documentation>
            <zeroOrMore>
                <choice>
                    <ref name="CHRONSTRUCT"/>
                    <ref name="KEYWORDCLASS"/>
                    <ref name="SHORTPROSE"/>
                </choice>
            </zeroOrMore>
            <optional>
                <ref name="RRESPONSES"/>
            </optional>
        </element>
    </define>

