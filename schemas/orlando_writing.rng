<!-- begin root <grammar> element -->
<grammar xmlns="http://relaxng.org/ns/structure/1.0"
    xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
    xmlns:xhtml="http://www.w3.org/1999/xhtml"
    datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes">

    <!-- begin <start> element pattern -->
    <start>

        <!-- begin WRITING element pattern -->
        <element name="WRITING">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Writing
                is the large, all-encompassing element in all writing documents.</a:documentation>
            <optional>
                <attribute name="ID">
                    <data type="NCName"/>
                </attribute>
            </optional>
            <attribute name="PERSON">
                <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                    >Person is an attribute applied to the Writing document as a whole, and
                    specifies information about the type of person who is the subject of the
                    document; for example, if they are known primarily as a writer, or a historical
                    figure.</a:documentation>
                <choice>
                    <value>HISTORICALFIGURE</value>
                    <value>WRITER</value>
                    <value>BRWWRITER</value>
                    <value>IBRWRITER</value>
                </choice>
            </attribute>
            <attribute name="SEX">
                <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Sex
                    is an attribute applied to the Writing document as a whole, and specifies
                    whether the individual who is the subject of the document is female, male,
                    transgendered, transgendered male-to-female, transgendered female-to-male, or
                    undefined.</a:documentation>
                <choice>
                    <value>FEMALE</value>
                    <value>MALE</value>
                    <value>TRANSGENDERED</value>
                    <value>TRANSGENDERED FEMALE-TO-MALE</value>
                    <value>TRANSGENDERED MALE-TO-FEMALE</value>
                    <value>UNDEFINED</value>
                    <data type="string"/>
                </choice>
            </attribute>

            <!-- begin ORLANDOHEADER element pattern -->
            <element name="ORLANDOHEADER">
                <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                    >Equivalent to TEI's TEIHeader, OrlandoHeader supplies the descriptive and
                    declarative information making up an electronic title page prefixed to every
                    TEI-conformant text.</a:documentation>
                <optional>
                    <attribute name="TYPE" a:defaultValue="text">
                        <a:documentation
                            xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Type
                            specifies the kind of document to which the header is attached, for
                            example whether it is a corpus or individual text. Sample values
                            include: 1] text; 2] corpus.</a:documentation>
                        <choice>
                            <value>text</value>
                            <value>corpus</value>
                            <data type="NCName"/>
                        </choice>
                    </attribute>
                </optional>
                <element name="FILEDESC">
                    <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                        >fileDesc contains a full bibliographic description of an electronic
                        file.</a:documentation>
                    <element name="TITLESTMT">
                        <a:documentation
                            xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">titleStmt
                            groups information about the title of a work and those responsible for
                            its intellectual content.</a:documentation>
                        <element name="DOCTITLE">
                            <a:documentation
                                xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                                >docTitle is an element in the OrlandoHeader that contains the title
                                of the document at hand for cataloguing purposes. This title will be
                                in the template for most of the documents created on the Orlando
                                Project, but will need to be filled in for certain documents, such
                                as events documents and documents created without a
                                template.</a:documentation>
                            <text/>
                        </element>
                    </element>
                    <element name="PUBLICATIONSTMT">
                        <a:documentation
                            xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                            >publicationStmt groups information concerning the publication or
                            distribution of an electronic or other text.</a:documentation>
                        <element name="AUTHORITY">
                            <a:documentation
                                xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">The
                                authority element contains the name of the person or organization
                                responsible for the construction of an XML file.</a:documentation>
                            <text/>
                        </element>
                    </element>
                    <element name="SOURCEDESC">
                        <a:documentation
                            xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">sourceDesc
                            describes the source from which an electronic text was derived or
                            generated, typically a bibliographic description in the case of a
                            digitized text, or a phrase such as "born digital" for a text which has
                            no previous existence.</a:documentation>
                        <text/>
                    </element>
                </element>
                <element name="REVISIONDESC">
                    <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                        >revisionDesc (or revision description) summarizes the revision history for
                        a file.</a:documentation>
                    <oneOrMore>
                        <element name="RESPONSIBILITY">
                            <a:documentation
                                xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">The
                                responsibility element records a particular workflow activity in the
                                workflow chain.</a:documentation>
                            <ref name="RESP-attribute"/>
                            <attribute name="WORKSTATUS" a:defaultValue="SUB">
                                <a:documentation
                                    xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                                    >Describes the work status of a document.</a:documentation>
                                <choice>
                                    <value type="token">SUB</value>
                                    <a:documentation>Submitted (SUB)</a:documentation>
                                    <value type="token">RWT</value>
                                    <a:documentation>Researched / Written / Tagged
                                        (RWT)</a:documentation>
                                    <value type="token">CAS</value>
                                    <a:documentation>Checked Against Sources (CAS)</a:documentation>
                                    <value type="token">RBV</value>
                                    <a:documentation>Reviewed by Volume Author
                                        (RVB)</a:documentation>
                                    <value type="token">CFT</value>
                                    <a:documentation>Checked for Tagging (CFT)</a:documentation>
                                    <value type="token">CFB</value>
                                    <a:documentation>Checked for Bibliographic Practices
                                        (CFB)</a:documentation>
                                    <value type="token">CFC</value>
                                    <!-- MRB: CFC? -->
                                    <value type="token">REV</value>
                                    <a:documentation>Revised (REV)</a:documentation>
                                    <value type="token">OLD</value>
                                    <!-- MRB: OLD? -->
                                    <value type="token">PUB</value>
                                    <a:documentation>Published (PUB)</a:documentation>
                                    <value type="token">ENH</value>
                                    <a:documentation>Enhanced (ENH)</a:documentation>
                                </choice>
                            </attribute>
                            <attribute name="WORKVALUE" a:defaultValue="I">
                                <a:documentation
                                    xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                                    >Records the completion information of a particular work status
                                    phase of a document.</a:documentation>
                                <choice>
                                    <value>I</value>
                                    <a:documentation>Incomplete</a:documentation>
                                    <value>P</value>
                                    <a:documentation>Pending</a:documentation>
                                    <value>C</value>
                                    <a:documentation>Complete</a:documentation>
                                    <value>O</value>
                                    <!-- MRB: O? -->
                                </choice>
                            </attribute>
                            <ref name="DATE"/>
                            <zeroOrMore>
                                <element name="ITEM">
                                    <a:documentation
                                        xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                                        >item contains one component of a list.</a:documentation>
                                    <text/>
                                </element>
                            </zeroOrMore>
                        </element>
                    </oneOrMore>
                </element>
            </element>
            <!-- end ORLANDOHEADER element pattern -->

            <!-- begin DIV0 element pattern -->
            <element name="DIV0">
                <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Div0
                    (i.e. level-0 text division) is the largest structural element permitted in any
                    document. XML works on the principle that all information in a document exists
                    hierarchically, or, stated otherwise, that all types of information can be
                    contained in other types of information. For CWRC projects, Div0 is the element
                    that contains everything in a document except the cataloguing information that
                    is contained in the CWRCHeader element.</a:documentation>
                <oneOrMore>
                    <choice>
                        <ref name="HEADING"/>
                        <ref name="RESEARCHNOTE"/>
                        <element name="AUTHORSUMMARY">
                            <a:documentation
                                xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">The
                                element authorSummary is specially devised for researchers to make
                                summary statements about an author before opening individual
                                production, reception, or textual features elements. You are
                                encouraged to use this element to preface the writing document of
                                each woman writer. It is useful both for minor writers about whom
                                little is known, and for major writers whose work needs to be
                                sumarized before launching into the details.</a:documentation>
                            <ref name="SHORTPROSE"/>
                            <optional>
                                <ref name="KEYWORDCLASS"/>
                            </optional>
                        </element>
                        <element name="DIV1">
                            <a:documentation
                                xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Div1
                                (i.e. level-1 text division) is an element that marks the second
                                largest structural division in a document.</a:documentation>
                            <optional>
                                <ref name="HEADING"/>
                            </optional>
                            <zeroOrMore>
                                <choice>
                                    <ref name="RESEARCHNOTE"/>
                                    <ref name="TEXTSCOPE"/>
                                    <element name="DIV2">
                                        <a:documentation
                                            xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                                            >Div2 (i.e. level-2 text division) is an element that
                                            marks the third largest structural division in a
                                            document.</a:documentation>
                                        <optional>
                                            <attribute name="ID">
                                                <data type="NCName"/>
                                            </attribute>
                                        </optional>
                                        <optional>
                                            <choice>
                                                <ref name="HEADING"/>
                                                <ref name="TITLE"/>
                                            </choice>
                                        </optional>
                                        <zeroOrMore>
                                            <choice>
                                                <ref name="KEYWORDCLASS"/>
                                                <ref name="PRODUCTION"/>
                                                <ref name="RECEPTION"/>
                                                <ref name="RESEARCHNOTE"/>
                                                <ref name="TEXTSCOPE"/>
                                                <ref name="TEXTUALFEATURES"/>
                                            </choice>
                                        </zeroOrMore>
                                    </element>
                                </choice>
                            </zeroOrMore>
                        </element>
                        <element name="STANDARD">
                            <a:documentation
                                xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                                >Standard is an element used to record the standard name by which a
                                woman writer is known. It prefaces the document and should reflect
                                the name under which she wrote or the name chosen as standard by the
                                    <em>Feminist Companion</em> or the <em>Drabble Oxford
                                    Companion</em>.</a:documentation>
                            <text/>
                        </element>
                        <element name="WORKSCITED">
                            <a:documentation
                                xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">The
                                WorksCited is an informal but complete list of all sources used in
                                the writing of documents. You should ensure that all of these
                                sources are also entered in the bibliography
                                database.</a:documentation>
                            <oneOrMore>
                                <choice>
                                    <ref name="RESEARCHNOTE"/>
                                    <element name="SOURCE">
                                        <a:documentation
                                            xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                                            >source describes the original source for the
                                            information contained with a manuscript
                                            description.</a:documentation>
                                        <oneOrMore>
                                            <choice>
                                                <text/>
                                                <ref name="RESEARCHNOTE"/>
                                                <ref name="SCHOLARNOTE"/>
                                                <ref name="TITLE"/>
                                            </choice>
                                        </oneOrMore>
                                    </element>
                                </choice>
                            </oneOrMore>
                        </element>
                    </choice>
                </oneOrMore>
                <optional>
                    <element name="BIOGPROSE">
                        <text/>
                    </element>
                </optional>
            </element>
            <!-- end DIV0 element pattern -->

        </element>
        <!-- end WRITING element pattern -->

    </start>
    <!-- end <start> element pattern -->

    <!-- *** begin globally defined elements *** -->

    <define name="DATE">
        <element name="DATE">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Date is
                one of three elements, along with dateRange and dateStruct, used to systematize the
                capture of dates across project documents. Date is used to tag all singular dates
                (not ranges) for which all components (day, month, year) have a common degree of
                certainty. Accurate and systematic tagging of dates on the project is a high
                priority both for the role that dates play in sorting and properly displaying
                chronology events, but also for facilitating the search and retrieval of
                non-chronology material across project documents.</a:documentation>
            <optional>
                <attribute name="CALENDAR" a:defaultValue="NEWSTYLE">
                    <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                        >An attribute of date, dateRange, and dateStruct, calendar is used to
                        indicate when a given date took place.</a:documentation>
                    <choice>
                        <value>NEWSTYLE</value>
                        <value>BC</value>
                    </choice>
                </attribute>
            </optional>
            <optional>
                <attribute name="CERTAINTY" a:defaultValue="CERT">
                    <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                        >Certainty is an attribute of date, dateRange, and dateStruct and is used to
                        indicate the nature of certainty that you have about a given
                        date.</a:documentation>
                    <choice>
                        <value>CERT</value>
                        <a:documentation>Certain</a:documentation>
                        <value>C</value>
                        <a:documentation>Circa</a:documentation>
                        <value>BY</value>
                        <a:documentation>By this date</a:documentation>
                        <value>AFTER</value>
                        <a:documentation>After this date</a:documentation>
                        <value>UNKNOWN</value>
                        <a:documentation>Unknown date</a:documentation>
                        <value>ROUGHLYDATED</value>
                        <a:documentation>Rough certainty</a:documentation>
                    </choice>
                </attribute>
            </optional>
            <optional>
                <attribute name="ID">
                    <data type="NCName"/>
                </attribute>
            </optional>
            <optional>
                <attribute name="VALUE">
                    <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                        >Value is used to record a formatted date-related value.</a:documentation>
                    <choice>
                        <data type="date"/>
                        <data type="string">
                            <param name="pattern">(\d{4})((\-(0[1-9]|1[0-2])\-))</param>
                        </data>
                        <data type="string">
                            <param name="pattern">(\d{4})(\-{2})</param>
                        </data>
                    </choice>
                </attribute>
            </optional>
            <oneOrMore>
                <choice>
                    <text/>
                    <ref name="RESEARCHNOTE"/>
                    <ref name="SIC"/>
                    <ref name="SOCALLED"/>
                    <ref name="TIME"/>
                </choice>
            </oneOrMore>
        </element>
    </define>

    <define name="HEADING">
        <element name="HEADING">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">head (or
                heading) contains any type of heading, for example the title of a section, or the
                heading of a list, glossary, manuscript description, etc.</a:documentation>
            <oneOrMore>
                <choice>
                    <text/>
                    <ref name="FOREIGN"/>
                    <ref name="QUOTE"/>
                    <ref name="RESEARCHNOTE"/>
                    <ref name="SOCALLED"/>
                    <ref name="TITLE"/>
                </choice>
            </oneOrMore>
        </element>
    </define>

    <define name="HEAD">
        <element name="HEAD">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">head
                contains a heading for the lg (line group) element and the figure (graphic
                information) element.</a:documentation>
            <text/>
        </element>
    </define>

    <define name="GRAPHIC">
        <element name="GRAPHIC">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Graphic
                indicates the location of an inline graphic, illustration, or
                figure.</a:documentation>
            <attribute name="URL">
                <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">URL
                    (uniform resource locator) specifies the URL from which the media concerned may
                    be obtained.</a:documentation>
                <data type="anyURI"/>
            </attribute>
        </element>
    </define>

    <define name="FIGURE">
        <element name="FIGURE">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">The
                figure element groups elements representing or containing graphic information such
                as an illustration, formula, or figure.</a:documentation>
            <oneOrMore>
                <choice>
                    <ref name="HEAD"/>
                    <element name="FIGDESC">
                        <a:documentation
                            xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">FigDesc
                            (description of figure) contains a brief prose description of the
                            appearance or content of a graphic figure, for use when documenting an
                            image without displaying it.</a:documentation>
                        <text/>
                    </element>
                    <ref name="GRAPHIC"/>
                    <ref name="P"/>
                </choice>
            </oneOrMore>
        </element>
    </define>

    <define name="RESEARCHNOTE">
        <element name="RESEARCHNOTE">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                >ResearchNote is available everywhere, and it allows us to comment among ourselves
                on the work at hand; not to be confused with scholarNotes that will be readable by
                end users, the contents of this element will be seen only by project
                members.</a:documentation>
            <oneOrMore>
                <choice>
                    <text/>
                    <ref name="BIBCITS"/>
                    <ref name="EMPH"/>
                    <ref name="FIGURE"/>
                    <ref name="GRAPHIC"/>
                    <ref name="NAME"/>
                    <ref name="ORGNAME"/>
                    <ref name="PAUTHORSHIP"/>
                    <ref name="PINFLUENCESHER"/>
                    <ref name="QUOTE"/>
                    <ref name="RESEARCHNOTE"/>
                    <ref name="SCHOLARNOTE"/>
                    <ref name="SOCALLED"/>
                    <ref name="TCHARACTERIZATION"/>
                    <ref name="TITLE"/>
                    <ref name="XREF"/>
                </choice>
            </oneOrMore>
        </element>
    </define>

    <define name="SHORTPROSE">
        <element name="SHORTPROSE">
            <optional>
                <choice>
                    <ref name="TPLOT"/>
                    <ref name="TSETTINGDATE"/>
                </choice>
            </optional>
            <oneOrMore>
                <choice>
                    <ref name="KEYWORDCLASS"/>
                    <ref name="P"/>
                    <ref name="RESEARCHNOTE"/>
                </choice>
            </oneOrMore>
            <optional>
                <choice>
                    <ref name="PARCHIVALLOCATION"/>
                    <ref name="RRESPONSES"/>
                </choice>
            </optional>
        </element>
    </define>

    <define name="KEYWORDCLASS">
        <element name="KEYWORDCLASS">
            <optional>
                <attribute name="KEYWORDTYPE"/>
            </optional>
            <text/>
        </element>
    </define>
    <define name="TEXTSCOPE">
        <element name="TEXTSCOPE">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">TextScope
                identifies the work under discussion in a Div 2. Structurally, TextScope resembles
                Bibcit in many ways: in a writing document, the TextScope tag itself is empty and
                most of the pertinent information is contained in attributes. Textscope has two key
                attributes: DBREF, which contains the dbref number of the corresponding bibliography
                item in the bibl database, and PLACEHOLDER, which contains a brief mention of the
                work under discussion and consists of the BWW's initials, a short title, and
                date.<br/> This element must be used in conjunction with a Div 2. You may not use it
                at the Div 1 level. You do not need to use a TextScope for each individual text you
                are discussing, or even for each text to which you devote an entire Div 2. Use the
                TextScope to demarcate discussions of major texts only. A useful rule of thumb is to
                consider whether you would enter the work under discussion in the bibliography
                database. If you would, then use a textscope for your discussion. Minor works would
                not be entered in the bibliography database and so do not require a textscope.<br/>
                The TextScope is not the same as a heading (and in fact may directly repeat your
                heading). Instead, the textScope demarcates the main discussion of a text for
                searching/indexing purposes. It is also used as a bibliographic pointer that will
                invisibly direct end-users to the bibliographic information about the text under
                discussion. The textScope tag will not appear in the reader's version of your
                document. It is an invisible tag for linking and searching
                purposes.</a:documentation>
            <optional>
                <attribute name="DBREF"/>
            </optional>
            <optional>
                <attribute name="PLACEHOLDER"/>
            </optional>
            <optional>
                <attribute name="REF">
                    <data type="anyURI"/>
                </attribute>
            </optional>
            <!-- 
            <oneOrMore>
                <choice>
                    <text/>
                    <ref name="PRODUCTION"/>
                    <ref name="RECEPTION"/>
                    <ref name="RESEARCHNOTE"/>
                    <ref name="TEXTUALFEATURES"/>
                </choice>
            </oneOrMore>
            -->
        </element>
    </define>

    <define name="TITLE">
