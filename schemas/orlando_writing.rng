                    <ref name="RESEARCHNOTE"/>
                    <ref name="SHORTPROSE"/>
                </choice>
            </zeroOrMore>
            <zeroOrMore>
                <ref name="KEYWORDCLASS"/>
            </zeroOrMore>
        </element>
    </define>

