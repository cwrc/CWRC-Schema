<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
  <xs:element name="cwrc">
    <xs:annotation>
      <xs:documentation>Group of CWRC records</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="entity"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="entity">
    <xs:annotation>
      <xs:documentation>CWRC entity record</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice>
        <xs:element ref="person"/>
        <xs:element ref="organization"/>
        <xs:element ref="place"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <xs:element name="person">
    <xs:annotation>
      <xs:documentation>CWRC person entity record</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="identity"/>
        <xs:group ref="personDescription-element"/>
        <xs:group ref="personRecordInfo-element"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <xs:element name="organization">
    <xs:annotation>
      <xs:documentation>CWRC organization entity record</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="identity"/>
        <xs:group ref="orgDescription-element"/>
        <xs:group ref="orgRecordInfo-element"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <xs:element name="place">
    <xs:annotation>
      <xs:documentation>CWRC place entity record</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="identity"/>
        <xs:group ref="placeDescription-element"/>
        <xs:group ref="placeRecordInfo-element"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <xs:element name="identity">
    <xs:annotation>
      <xs:documentation>A container element grouping data for identification of a CWRC entity
                instance</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="preferredForm"/>
        <xs:element ref="displayLabel"/>
        <xs:element ref="variantForms"/>
        <xs:element ref="sameAs"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <xs:group name="personDescription-element">
    <xs:sequence>
      <xs:element name="description">
        <xs:annotation>
          <xs:documentation>This section captures descriptive data about a CWRC person entity
                record</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:element ref="existDates"/>
            <xs:element ref="factuality"/>
            <xs:element ref="genders"/>
            <xs:element ref="occupations"/>
            <xs:element ref="activities"/>
            <xs:element ref="researchInterests"/>
            <xs:element ref="descriptiveNotes"/>
            <xs:element ref="relatedResources"/>
          </xs:choice>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:group>
  <xs:group name="orgDescription-element">
    <xs:sequence>
      <xs:element name="description">
        <xs:annotation>
          <xs:documentation>This section captures descriptive data about a CWRC organization entity
                record</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:element ref="existDates"/>
            <xs:element ref="factuality"/>
            <xs:element ref="descriptiveNotes"/>
            <xs:element ref="relatedResources"/>
          </xs:choice>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:group>
  <xs:group name="placeDescription-element">
    <xs:sequence>
      <xs:element name="description">
        <xs:annotation>
          <xs:documentation>This section captures descriptive data about a CWRC place entity
                record</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:element ref="existDates"/>
            <xs:element ref="factuality"/>
            <xs:element ref="latitude"/>
            <xs:element ref="longitude"/>
            <xs:element ref="featureClass"/>
            <xs:element ref="featureCodeLabel"/>
            <xs:element ref="countryName"/>
            <xs:element ref="firstAdministrativeDivision"/>
            <xs:element ref="secondAdministrativeDivision"/>
            <xs:element ref="elevation"/>
            <xs:element ref="averageElevation"/>
            <xs:element ref="timeZoneOffset"/>
            <xs:element ref="descriptiveNotes"/>
            <xs:element ref="relatedResources"/>
          </xs:choice>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:group>
  <xs:group name="personRecordInfo-element">
    <xs:sequence>
      <xs:element name="recordInfo">
        <xs:annotation>
          <xs:documentation>Captures record-level information. Components of this element are kept
                to a minimum given that administrative and object-level metadata is captured in
                other FOXML datastreams.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:element ref="personTypes"/>
            <xs:element ref="originInfo"/>
            <xs:element ref="entityId"/>
            <xs:element ref="accessCondition"/>
          </xs:choice>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:group>
  <xs:group name="orgRecordInfo-element">
    <xs:sequence>
      <xs:element name="recordInfo">
        <xs:annotation>
          <xs:documentation>Captures record-level information. Components of this element are kept
                to a minimum given that administrative and object-level metadata is captured in
                other FOXML datastreams.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:element ref="orgTypes"/>
            <xs:element ref="originInfo"/>
            <xs:element ref="entityId"/>
            <xs:element ref="accessCondition"/>
          </xs:choice>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:group>
  <xs:group name="placeRecordInfo-element">
    <xs:sequence>
      <xs:element name="recordInfo">
        <xs:annotation>
          <xs:documentation>Captures record-level information. Components of this element are kept
                to a minimum given that administrative and object-level metadata is captured in
                other FOXML datastreams.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:element ref="placeTypes"/>
            <xs:element ref="originInfo"/>
            <xs:element ref="entityId"/>
            <xs:element ref="accessCondition"/>
          </xs:choice>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:group>
  <xs:element name="preferredForm">
    <xs:annotation>
      <xs:documentation>An element used to indicate which recorded entity name is the preferred
                one for display purposes in a given context.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="namePart"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="displayLabel" type="xs:string">
    <xs:annotation>
      <xs:documentation>Display label of entity described if different than CWRC's
                preferredForm.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="namePart">
    <xs:annotation>
      <xs:documentation>Used to distinguish components of the name of the entity. Equivalent to
                MADS's namePart and to EAC-CPF's part element.</xs:documentation>
    </xs:annotation>
    <xs:complexType mixed="true">
      <xs:attribute name="partType">
        <xs:annotation>
          <xs:documentation>Captures types of name components of person entities. Example:
                family.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:union>
            <xs:simpleType>
              <xs:restriction base="xs:token">
                <xs:enumeration value="fullNameInvertedOrder">
                  <xs:annotation>
                    <xs:documentation>e.g., Abergavenny, Frances Neville, Baroness</xs:documentation>
                  </xs:annotation>
                </xs:enumeration>
                <xs:enumeration value="fullNameDirectOrder">
                  <xs:annotation>
                    <xs:documentation>e.g., Baroness Frances Neville Abergavenny</xs:documentation>
                  </xs:annotation>
                </xs:enumeration>
                <xs:enumeration value="family">
                  <xs:annotation>
                    <xs:documentation>e.g., Abergavenny</xs:documentation>
                  </xs:annotation>
                </xs:enumeration>
                <xs:enumeration value="given">
                  <xs:annotation>
                    <xs:documentation>e.g., Frances Neville</xs:documentation>
                  </xs:annotation>
                </xs:enumeration>
                <xs:enumeration value="termsOfAddress">
                  <xs:annotation>
                    <xs:documentation>e.g., Baroness</xs:documentation>
                  </xs:annotation>
                </xs:enumeration>
              </xs:restriction>
            </xs:simpleType>
            <xs:simpleType>
              <xs:restriction base="xs:token">
                <xs:length value="0"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:union>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="partType-attribute">
    <xs:attribute name="partType" use="required">
      <xs:annotation>
        <xs:documentation>Captures types of name components of person entities. Example:
                family.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:union>
          <xs:simpleType>
            <xs:restriction base="xs:token">
              <xs:enumeration value="fullNameInvertedOrder">
                <xs:annotation>
                  <xs:documentation>e.g., Abergavenny, Frances Neville, Baroness</xs:documentation>
                </xs:annotation>
              </xs:enumeration>
              <xs:enumeration value="fullNameDirectOrder">
                <xs:annotation>
                  <xs:documentation>e.g., Baroness Frances Neville Abergavenny</xs:documentation>
                </xs:annotation>
              </xs:enumeration>
              <xs:enumeration value="family">
                <xs:annotation>
                  <xs:documentation>e.g., Abergavenny</xs:documentation>
                </xs:annotation>
              </xs:enumeration>
              <xs:enumeration value="given">
                <xs:annotation>
                  <xs:documentation>e.g., Frances Neville</xs:documentation>
                </xs:annotation>
              </xs:enumeration>
              <xs:enumeration value="termsOfAddress">
                <xs:annotation>
                  <xs:documentation>e.g., Baroness</xs:documentation>
                </xs:annotation>
              </xs:enumeration>
            </xs:restriction>
          </xs:simpleType>
          <xs:simpleType>
            <xs:restriction base="xs:token">
              <xs:length value="0"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:union>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:element name="variantForms">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="variant"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="variant">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="namePart"/>
        <xs:element ref="variantType"/>
        <xs:element ref="authorizedBy"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <xs:element name="variantType">
    <xs:annotation>
      <xs:documentation>Captures name type to describe contexts in which this name was used to
                refer to the present entity.</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:restriction base="xs:token">
        <xs:enumeration value="birthName"/>
        <xs:enumeration value="marriedName"/>
        <xs:enumeration value="indexedName"/>
        <xs:enumeration value="pseudonym"/>
        <xs:enumeration value="usedForm"/>
        <xs:enumeration value="nickname"/>
        <xs:enumeration value="religiousName"/>
        <xs:enumeration value="royalName"/>
        <xs:enumeration value="selfConstructedName"/>
        <xs:enumeration value="styledName"/>
        <xs:enumeration value="titledName"/>
        <xs:enumeration value="orlandoStandardName"/>
        <xs:enumeration value="birthName"/>
        <xs:enumeration value="descriptive">
          <xs:annotation>
            <xs:documentation>e.g., Women's Aid Society</xs:documentation>
          </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="popular">
          <xs:annotation>
            <xs:documentation>e.g., "Wrens" for "Women's Royal Naval Service"</xs:documentation>
          </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="acronym">
          <xs:annotation>
            <xs:documentation>e.g., OCLC</xs:documentation>
          </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="alternate names">
          <xs:annotation>
            <xs:documentation>e.g.,
                    Edmonton,Edmontona,Edmontonas,Edmontono,Edmundopolis,Edmuntun,Ehdmonton</xs:documentation>
          </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="deprecated"/>
        <xs:enumeration value="historic"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="authorizedBy">
    <xs:annotation>
      <xs:documentation>authorizedBy element captures a note indicating that one or more
                projects use the present name as a preferred form in their local
                context.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="projectId"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="sameAs">
    <xs:annotation>
      <xs:documentation>Enter a URI to a linked data resource that is the same as this entity
                (e.g., a VIAF record).</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:simpleContent>
        <xs:extension base="xs:anyURI">
          <xs:attribute name="cert">
            <xs:annotation>
              <xs:documentation>Captures degree of certainty about a particular statement about the
                entity described.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
              <xs:union>
                <xs:simpleType>
                  <xs:restriction base="xs:token">
                    <xs:enumeration value="4"/>
                    <xs:enumeration value="3"/>
                    <xs:enumeration value="2"/>
                    <xs:enumeration value="1"/>
                  </xs:restriction>
                </xs:simpleType>
                <xs:simpleType>
                  <xs:restriction base="xs:token">
                    <xs:length value="0"/>
                  </xs:restriction>
                </xs:simpleType>
              </xs:union>
            </xs:simpleType>
          </xs:attribute>
        </xs:extension>
      </xs:simpleContent>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="cert-attribute">
    <xs:attribute name="cert" use="required">
      <xs:annotation>
        <xs:documentation>Captures degree of certainty about a particular statement about the
                entity described.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:union>
          <xs:simpleType>
            <xs:restriction base="xs:token">
