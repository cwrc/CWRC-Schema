  <xs:element name="orgType">
    <xs:annotation>
      <xs:documentation>Captures generic organization types. Based on EAC-CPF's
                @localType.</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:union>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="labour"/>
            <xs:enumeration value="club or society"/>
            <xs:enumeration value="political"/>
            <xs:enumeration value="academic"/>
            <xs:enumeration value="research"/>
            <xs:enumeration value="corporate"/>
            <xs:enumeration value="activist"/>
            <xs:enumeration value="governmental"/>
            <xs:enumeration value="military"/>
            <xs:enumeration value="law-enforcement"/>
            <xs:enumeration value="professional"/>
            <xs:enumeration value="sporting"/>
            <xs:enumeration value="medical"/>
            <xs:enumeration value="altruistic"/>
            <xs:enumeration value="non-profit"/>
            <xs:enumeration value="artistic"/>
            <xs:enumeration value="dramatic"/>
            <xs:enumeration value="ethnic"/>
            <xs:enumeration value="literary"/>
            <xs:enumeration value="musical"/>
            <xs:enumeration value="religious"/>
            <xs:enumeration value="publishing"/>
            <xs:enumeration value="school"/>
          </xs:restriction>
        </xs:simpleType>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:length value="0"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:union>
    </xs:simpleType>
  </xs:element>
  <xs:element name="placeTypes">
    <xs:annotation>
      <xs:documentation>Captures generic place types defined by CWRC.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="placeType"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="placeType">
    <xs:annotation>
      <xs:documentation>Captures generic place types defined by CWRC.</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:restriction base="xs:token">
        <xs:enumeration value="currentHumanPlace"/>
        <xs:enumeration value="historicalHumanPlace"/>
        <xs:enumeration value="currentNaturalPlace"/>
        <xs:enumeration value="historicalNaturalPlace"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="originInfo">
    <xs:annotation>
      <xs:documentation>Record origin information. Equivalent to MADS recordInfo element,
                similar to EAC-CPF's control element.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="projectId"/>
        <xs:element ref="recordIdentifier"/>
        <xs:element ref="recordCreationDate"/>
        <xs:element ref="recordChangeDate"/>
        <xs:element ref="recordOrigin"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <xs:element name="projectId">
    <xs:annotation>
      <xs:documentation>Similar to EAC-CPF's agencyCode. In CWRC's context, projectId captures
                an identifier for the originating project. Subsequent modifications of the record
                should be captured in FOXML's datastreams</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:union>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="canwwr">
              <xs:annotation>
                <xs:documentation>canwwwr</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="ceww">
              <xs:annotation>
                <xs:documentation>ceww</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="cwrc">
              <xs:annotation>
                <xs:documentation>cwrc</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="eccji">
              <xs:annotation>
                <xs:documentation>eccji</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="emic">
              <xs:annotation>
                <xs:documentation>emic</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="GeoNames">
              <xs:annotation>
                <xs:documentation>GeoNames</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Google Maps">
              <xs:annotation>
                <xs:documentation>Google Maps</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="orlando">
              <xs:annotation>
                <xs:documentation>orlando</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="VIAF">
              <xs:annotation>
                <xs:documentation>VIAF</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:length value="0"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:union>
    </xs:simpleType>
  </xs:element>
  <xs:element name="recordIdentifier">
    <xs:annotation>
      <xs:documentation>Provenance field used to capture original record identifiers. CWRC
                record identifiers are captured in entityId and in FOXML.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:simpleContent>
        <xs:extension base="xs:string">
          <xs:attribute name="source" type="xs:string">
            <xs:annotation>
              <xs:documentation>Contains the code or name of the organization whose system
                        control number is located in the recordIdentifier element.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
        </xs:extension>
      </xs:simpleContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="recordCreationDate">
    <xs:annotation>
      <xs:documentation>Provenance field used to capture the original creation date of the
                entity or authority record. Equivalent to MADS recordCreationDate and EAC-CPF's
                maintenanceHistory/maintenance/Event/eventType[text()="created"]/eventDateTime .
                Date format is in the form of YYYY, YYYY-MM, or YYYY-MM-DD format.</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:union>
        <xs:simpleType>
          <xs:union memberTypes="xs:date xs:gYearMonth xs:gYear"/>
        </xs:simpleType>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:length value="0"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:union>
    </xs:simpleType>
  </xs:element>
  <xs:element name="recordChangeDate">
    <xs:annotation>
      <xs:documentation>Provenance field used to capture the last modification date of an
                entity record. Equivalent to MADS recordChangeDate and EAC-CPF's
                maintenanceHistory/maintenance/Event/eventType/eventDateTime . Date format is in the
                form of YYYY, YYYY-MM, or YYYY-MM-DD format.</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:union>
        <xs:simpleType>
          <xs:union memberTypes="xs:date xs:gYearMonth xs:gYear"/>
        </xs:simpleType>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:length value="0"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:union>
    </xs:simpleType>
  </xs:element>
  <xs:element name="recordOrigin" type="xs:string">
    <xs:annotation>
      <xs:documentation>Field that records the origin or provenance of the entity
                record.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="entityId">
    <xs:annotation>
      <xs:documentation>Optional CWRC record identifier. This information is captured in FOXML.
                Equivalent to EAC-CPF's recordId.</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:union memberTypes="xs:string">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:length value="0"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:union>
    </xs:simpleType>
  </xs:element>
  <xs:element name="accessCondition">
    <xs:annotation>
      <xs:documentation>Use the following access condition statement: Use of this public-domain
                resource is governed by the &lt;a
                href="http://creativecommons.org/licenses/by-nc/4.0/" rel="license" &gt;Creative
                Commons Attribution-NonCommercial 4.0 International License&lt;/a&gt;. </xs:documentation>
    </xs:annotation>
    <xs:complexType mixed="true">
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="a"/>
      </xs:sequence>
      <xs:attribute name="type" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="use and reproduction"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="a">
    <xs:complexType>
      <xs:simpleContent>
        <xs:extension base="xs:token">
          <xs:attributeGroup ref="href-attribute"/>
          <xs:attribute name="rel" type="xs:token"/>
        </xs:extension>
      </xs:simpleContent>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="href-attribute">
    <xs:attribute name="href" use="required" type="xs:anyURI"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="rel-attribute">
    <xs:attribute name="rel" use="required" type="xs:token"/>
  </xs:attributeGroup>
</xs:schema>
