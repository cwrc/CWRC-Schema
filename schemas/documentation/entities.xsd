              <xs:enumeration value="3"/>
              <xs:enumeration value="2"/>
              <xs:enumeration value="1"/>
            </xs:restriction>
          </xs:simpleType>
          <xs:simpleType>
            <xs:restriction base="xs:token">
              <xs:length value="0"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:union>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:element name="existDates">
    <xs:annotation>
      <xs:documentation>A container element for the dateSingle element.</xs:documentation>
    </xs:annotation>
