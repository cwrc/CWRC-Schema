            <xs:enumeration value="RECENT"/>
            <xs:enumeration value="INITIAL"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="RS">
