    &lt;xs:sequence&gt;
      &lt;xs:element minOccurs="0" ref="CHRONSTRUCT"/&gt;
      &lt;xs:choice minOccurs="0"&gt;
        &lt;xs:element ref="BIRTHPOSITION"/&gt;
        &lt;xs:element ref="RESEARCHNOTE"/&gt;
      &lt;/xs:choice&gt;
      &lt;xs:element minOccurs="0" maxOccurs="unbounded" ref="DIV2"/&gt;
    &lt;/xs:sequence&gt;
  &lt;/xs:complexType&gt;
&lt;/xs:element&gt;</codeblock>
