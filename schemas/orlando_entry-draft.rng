                    <ref name="PMATERIALCONDITIONS"/>
                    <ref name="PMODEOFPUBLICATION"/>
                    <ref name="PMOTIVES"/>
                    <ref name="PPERFORMANCE"/>
                    <ref name="PRARITIESFEATURESDECORATIONS"/>
                    <ref name="PSERIALIZATION"/>
                    <ref name="PSUBMISSIONSREJECTIONS"/>
                    <ref name="QUOTE"/>
                    <ref name="RESEARCHNOTE"/>
                    <ref name="RLANDMARKTEXT"/>
                    <ref name="RPENALTIES"/>
                    <ref name="RRESPONSES"/>
